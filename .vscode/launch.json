{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Hosted workbench",
      "type": "chrome",
      "request": "launch",
      "url": "https://cjinter.sharepoint.com/sites/newportal/_layouts/workbench.aspx?debug=true&noredir=true&debugManifestsFile=https://localhost:4321/temp/manifests.js",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///.././src/*": "${webRoot}/src/*",
        "webpack:///../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../../src/*": "${webRoot}/src/*"
      },
      "runtimeArgs": ["--remote-debugging-port=9222", "-incognito"]
    }
  ]
}
